
{
	"root": true,
	"parser": "@typescript-eslint/parser",
	"parserOptions": {
		"project": "./tsconfig.eslint.json"
	},
	"plugins": [
		"@typescript-eslint"
	],
	"extends": [
		"airbnb-typescript/base"
	],
		"rules": {
			"max-len": [
			"error",
			{
				"code": 200,
				"comments": 180
			}
			]
		}
	}
